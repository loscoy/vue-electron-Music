{
  "name": "my-vue-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "start": "kill-port 3920 && concurrently -k \"vite\" \"wait-on tcp:3920 && npm-run-all watch\"",
    "start:ect": "cross-env NODE_ENV=development electron ./output/build/main.js",
    "watch": "tsc-watch --noClear -p tsconfig.e.json --onSuccess \"npm-run-all start:ect\""
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "axios": "^0.27.2",
    "element-plus": "^1.0.2-beta.28",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "ant-design-vue": "^2.2.8",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron": "^18.2.2",
    "electron-builder": "^23.0.3",
    "kill-port": "^1.6.1",
    "npm-run-all": "^4.1.5",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-icons": "^0.14.3",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.7",
    "vue-axios": "^3.4.1",
    "vue-tsc": "^0.34.7",
    "wait-on": "^6.0.1"
  }
}
